# usage
# tmuxifier load-window inhouse 

# Set window root path. Default is `$session_root`.
# Must be called before `new_window`.
window_root "~/code/nhatrang-webapp"

# Create new window. If no argument is given, window name will be based on
# layout file name.
new_window "inhouse-agit"

# Split window into panes.
#split_v 75
#split_v 66
#split_v 50

split_v 16
select_pane 1

split_v 20
select_pane 1

split_v 25
select_pane 1

split_v 33
select_pane 1

split_v 50

# split window finish

select_pane 1
run_cmd 'goto dubai004'

select_pane 2
run_cmd 'goto dubai005'

select_pane 3
run_cmd 'goto dubai012'

select_pane 4
run_cmd 'goto dubai013'

select_pane 5
run_cmd 'goto dubai017'

select_pane 6
run_cmd 'goto dubai021'

# Set active pane.
