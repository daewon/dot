# dot-managed Prezto configuration.
# Keep the default set and add interactive modules for git/history UX.
zstyle ':prezto:*:*' color 'yes'

zstyle ':prezto:load' pmodule \
  'environment' \
  'terminal' \
  'editor' \
  'history' \
  'history-substring-search' \
  'directory' \
  'spectrum' \
  'utility' \
  'completion' \
  'command-not-found' \
  'autosuggestions' \
  'git' \
  'prompt' \
  'syntax-highlighting'
